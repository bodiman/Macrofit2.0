{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@shared/*": ["./../shared/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "./styled.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
